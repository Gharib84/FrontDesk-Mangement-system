{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "builds": [
        {
            "src": "build/**/*",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "build/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}